package com.bogboa.model;
import java.util.List;
import java.util.Scanner;
import java.util.ArrayList;

// Define Participant class
class Participant {
    private int id;
    private boolean participation=true;
    private boolean participationResult=false;
    private int BOG;
    private int BOA;
    private boolean BOGusability= true;
    private int banDays=0;
    private int bogUsed=0;
    private int solde=0;
    private boolean participationRequest=true;
    private int chances=0;
    

    public boolean getParticipation() {
		return participation;
	}



	public void setParticipation(boolean participation) {
		this.participation = participation;
	}



	public boolean getParticipationResult() {
		return participationResult;
	}



	public void setParticipationResult(boolean participationResult) {
		this.participationResult = participationResult;
	}



	public int getBOG() {
		return BOG;
	}



	public void setBOG(int bOG) {
		BOG = bOG;
	}



	public boolean getBOGusability() {
		return BOGusability;
	}



	public void setBOGusability(boolean bOGusability) {
		BOGusability = bOGusability;
	}



	public void setId(int id) {
		this.id = id;
	}



	public int getId() {
        return id;
    }


	public int getBanDays() {
		return banDays;
	}



	public void setBanDays(int banDays) {
		this.banDays = banDays;
	}



	public int getBogUsed() {
		return bogUsed;
	}



	public void setBogUsed(int bogUsed) {
		this.bogUsed = bogUsed;
	}



	public int getSolde() {
		return solde;
	}



	public void setSolde(int solde) {
		this.solde = solde;
	}




	public boolean getParticipationRequest() {
		return participationRequest;
	}



	public void setParticipationRequest(boolean participationRequest) {
		this.participationRequest = participationRequest;
	}


	public int getBOA() {
		return BOA;
	}



	public void setBOA(int bOA) {
		BOA = bOA;
	}



	public int getChances() {
		return chances;
	}



	public void setChances(int chances) {
		this.chances = chances;
	}



	public Participant(int id, boolean participation, boolean participationResult, int bOG, int bOA,
			boolean bOGusability, int banDays, int bogUsed, int solde, boolean participationRequest, int chances) {
		super();
		this.id = id;
		this.participation = participation;
		this.participationResult = participationResult;
		BOG = bOG;
		BOA = bOA;
		BOGusability = bOGusability;
		this.banDays = banDays;
		this.bogUsed = bogUsed;
		this.solde = solde;
		this.participationRequest = participationRequest;
		this.chances = chances;
	}
	
	
}

